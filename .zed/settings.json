{
  "code_actions_on_format": {
    "source.fixAll.eslint": true,
    "source.removeUnusedImports": true
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "cargo": {
          "extraEnv": {}
        },
        "server": {
          "extraEnv": {}
        },
        "runnables": {
          "extraEnv": {}
        }
      }
    }
  },
  "terminal": {
    "env": {}
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  }
}
